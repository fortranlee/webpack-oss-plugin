{
  "name": "webpack-oss-plugin",
  "version": "0.9.3",
  "description": "Uploads compiled assets to oss after build",
  "main": "dist/oss_plugin.js",
  "scripts": {
    "build": "webpack",
    "test": "NODE_ENV='test' mocha -t 10000 --compilers js:babel-register --require babel-polyfill",
    "lint": "eslint ./src ./test",
    "watch": "webpack --watch",
    "prep:major": "npm run build && npm version major",
    "prep:minor": "npm run build && npm version minor",
    "prep:patch": "npm run build && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:waijule/webpack-oss-plugin.git"
  },
  "keywords": [
    "oss",
    "webpack",
    "node",
    "upload",
    "production"
  ],
  "author": "Jack Feng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waijule/webpack-oss-plugin/issues"
  },
  "homepage": "https://github.com/waijule/webpack-oss-plugin",
  "dependencies": {
    "ali-oss": "^4.4.4",
    "co": "^4.6.0",
    "lodash": "^4.13.0",
    "recursive-readdir": "^2.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.4.1",
    "dotenv": "^2.0.0",
    "eslint": "^2.13.0",
    "eslint-loader": "^1.3.0",
    "html-webpack-plugin": "^2.21.0",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  }
}
